{
 "info": {"version": 1, "license": "GPL-2.0-or-later"},
 "layout": {
  "type": "vbox",
  "widgets": [
   {"type": "label", "text": "Label", "tattr": "large"},
   {"type": "checkbox", "label": "Check Box", "set": true},
   {"type": "pbar", "val": 50, "uid": "pbar"},
   {"type": "slider", "min": 0, "max": 100, "val": 50, "on_event": "slider_event"},
   {"type": "tbox", "text": "Text Box", "len": 18},
   {"type": "tbox", "len": 18, "ttype": "hidden", "uid": "passwd", "help": "Password"},
   {"type": "tbox", "text": "http://example.com/index.html", "ttype": "URL"},
   {"type": "spinner", "min": 0, "max": 10, "val": 5},
   {"type": "radiobutton",
    "choices": [
     "First",
     "Second",
     "Third"
    ],
    "selected": 1},
   {"type": "color_scheme_switch"},
   {"type": "button", "label": "Button", "on_event": "button_on_event"}
  ]
 }
}
